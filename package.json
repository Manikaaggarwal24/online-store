{
  
  "homepage": "https://manikaaggarwal24.github.io/online-store/",
  "name": "online-store",
  "version": "2.0.0",
  "description": "online store with MERN",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install  && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "manika",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "dotenv": "^16.3.1",
    "gh-pages": "^6.0.0",
    "nodemon": "^3.0.1"
  }
}
